@article{Webbink2007,
  title = {Returns to University Education: Evidence from a {{Dutch}} Institutional Reform},
  author = {Webbink, D},
  date = {2007},
  journaltitle = {Economica},
  volume = {74},
  pages = {113--134},
  publisher = {{Wiley Online Library}},
  abstract = {In 1982 the duration of university education in the Netherlands was
reduced from five to four years. This institutional reform is here
used to estimate the causal effect of one year of university education
