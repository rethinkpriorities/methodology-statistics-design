@article{Fernandes2014,
  title = {Financial {{Literacy}}, {{Financial Education}}, and {{Downstream Financial Behaviors}}},
  author = {Fernandes, Daniel and Lynch, John G. and Netemeyer, Richard G.},
  date = {2014},
  journaltitle = {Management Science},
  volume = {60},
  pages = {1861--1883},
  issn = {0025-1909},
  doi = {10.1287/mnsc.2013.1849},
  url = {http://pubsonline.informs.org/doi/abs/10.1287/mnsc.2013.1849},
  abstract = {Policy makers have embraced financial education as a necessary antidote to the increasing complexity of\textbackslash{}r\textbackslash{}nconsumers’ financial decisions over the last generation. We conduct a meta-analysis of the relationship of\textbackslash{}r\textbackslash{}nfinancial literacy and of financial education to financial behaviors in 168 papers covering 201 prior\textbackslash{}r\textbackslash{}nstudies. We find that interventions to improve financial literacy explain only 0.1\% of the variance in\textbackslash{}r\textbackslash{}nfinancial behaviors studied, with weaker effects in low-income samples. Like other education, financial\textbackslash{}r\textbackslash{}neducation decays over time; even large interventions with many hours of instruction have negligible\textbackslash{}r\textbackslash{}neffects on behavior 20 months or more from the time of intervention. Correlational studies that measure\textbackslash{}r\textbackslash{}nfinancial literacy find stronger associations with financial behaviors. We conduct three empirical studies,\textbackslash{}r\textbackslash{}nand we find that the partial effects of financial literacy diminish dramatically when one controls for\textbackslash{}r\textbackslash{}npsychological traits that have been omitted in prior research or when one uses an instrument for financial\textbackslash{}r\textbackslash{}nliteracy to control for omitted variables. Financial education as studied to date has serious limitations that\textbackslash{}r\textbackslash{}nhave been masked by the apparently larger effects in correlational studies. We envisage a reduced role\textbackslash{}r\textbackslash{}nfor financial education that is not elaborated or acted upon soon afterward. We suggest a real but\textbackslash{}r\textbackslash{}nnarrower role for “just in time” financial education tied to specific behaviors it intends to help. We\textbackslash{}r\textbackslash{}nconclude with a discussion of the characteristics of behaviors that might affect the policy maker’s mix of\textbackslash{}r\textbackslash{}nfinancial education, choice architecture, and regulation as tools to help consumer financial behavior.},
  archivePrefix = {arXiv},
  eprint = {25246403},
  eprinttype = {pmid},
  isbn = {0025-1909},
  keywords = {behavioral economics,consumer behavior,education systems,household finance,public policy},
  number = {8}
}

