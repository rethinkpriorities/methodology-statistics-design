@article{Mellers1997,
  title = {Decision Affect Theory: {{Emotional}} Reactions to the Outcomes of Risky Options},
  author = {Mellers, B A and Schwartz, A and Ho, K and Ritov, I},
  date = {1997},
  journaltitle = {Psychological Science},
  pages = {423--429},
  publisher = {{JSTOR}},
  abstract = {Examined emotional experiences associated with the outcomes of risky
options represented as gambles with monetary consequences. In Exp
1 with 67 undergraduates (aged 18–26 yrs), 25 gambles were presented
to Ss: In Exp 2, with 47 undergraduates (aged 18–27 yrs), were presented
45 gambles to Ss. Results from the 2 experiments show that the emotional
reaction to a monetary outcome is not a simple function of the utility
of that outcome. Emotional responses also depend on probabilities
and unobtained outcomes. Unexpected outcomes have greater emotional
impact than expected outcomes. Furthermore, any given outcome is
less pleasant if an unobtained outcome is better. The authors propose
an account of emotional experiences associated with outcomes of decisions
called decision affect theory. Decision affect theory incorporates
utilities, expectations, and counterfactual comparisons into hedonic
responses. Finally, the authors show that choices between risky options
can be described as the maximization of expected emotional experiences,
as predicted by decision affect theory. (PsycINFO Database Record
(c) 2012 APA, all rights reserved)},
  keywords = {behavioral,effect,emotion,giving and probability,happiness,philanthropy,psychological,risk}
}

