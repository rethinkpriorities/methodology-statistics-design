  author = {Andrews, Kyle R. and Carpenter, Christopher J. and Shaw, Allison S. and Boster, Franklin J.},
  date = {2008},
  journaltitle = {Communication Reports},
  volume = {21},
  pages = {59--69},
  issn = {08934215},
  doi = {10.1080/08934210802305028},
  abstract = {A meta-analysis was conducted on the legitimization of paltry favors (LPF) effect (Cialdini \&amp; Schroeder, 1976). A total of 19 studies met the inclusion criteria, with a combined sample of 2,730 subjects. Excluding studies in which the LPF request was delivered via mail and those studies that accepted pledges as the dependent variable resulted in a homogeneous set of effect sizes (\emph{r}~=~.18, \emph{OR}~=~2.41). While the data provide clues as to possible mediating mechanisms, the cause of the effect is still not clear. Directions for future research are suggested.},
  file = {files/8161/lpf_effect_metaanal.pdf},
  keywords = {Compliance-gaining,Even a penny will help,Legitimization of paltry favors},
  number = {2}
}

