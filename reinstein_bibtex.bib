@article{Caplin2003,
  title = {{{AIDS Policy}} and {{Psychology}}: {{A Mechanism}}-{{Design Approach}}},
  author = {Caplin, Andrew and Eliaz, Kfir},
  date = {2003},
  journaltitle = {The RAND Journal of Economics},
  volume = {34},
  pages = {631--646},
  publisher = {{Blackwell Publishing on behalf of The RAND Corporation}},
  issn = {07416261},
  abstract = {Economic theorists have given little attention to health-related externalities,
such as those involved in the spread of AIDS. One reason for this
is the critical role played by psychological factors, such as fear
of testing, in the continued spread of the disease. We develop a
model of AIDS transmission that acknowledges this form of fear. In
this context we design a mechanism that not only encourages testing
but also slows the spread of the disease through voluntary transmission.
Our larger agenda is to demonstrate the power of psychological incentives
in the public health arena.},
  eprint = {1593781},
  eprinttype = {jstor},
  keywords = {aids,behavioral,health,mechanism design,pop,psychology},
  number = {4}
}

