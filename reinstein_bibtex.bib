@article{Lankford1991,
  title = {Modeling {{Charitable Giving Using}} a {{Box}}-{{Cox Standard Tobit Model}}},
  author = {Lankford, R Hamilton and Wyckoff, James H},
  date = {1991-08},
  journaltitle = {The Review of Economics and Statistics},
  volume = {73},
  pages = {460--470},
  publisher = {{The MIT Press}},
  issn = {0034-6535},
  url = {http://links.jstor.org/sici?sici=0034-6535(199108)73:3%3C460:MCGUAB%3E2.0.CO;2-R},
  abstract = {There is a large literature investigating the determinants of charitable
giving, in particular, price and income elasticities. We find that
many of the most often reported results from this literature rely
on a model specification that is inappropriate when applied to recent
Federal Tax File data. We develop a standard Tobit model with a Box-Cox
transformation parameter that nests a number of other specifications.
The Box-Cox model yields estimates of price elasticities and expected
charitable contributions that differ from those obtained using the
typical specification. Additionally, we find that the use of sample
weights provides potentially important policy insights.},
  keywords = {applied econometrics,corner solutions,econometrics,empirical,philanthropy,robust methods,tobit},
  number = {3}
}

