@article{andreoniWhichFairSex2001,
  title = {Which Is the Fair Sex? \{\vphantom\}{{G}}\vphantom\{\}ender Differences in Altruism},
  author = {Andreoni, James and Vesterlund, Lise},
  date = {2001},
  journaltitle = {Quarterly Journal of Economics},
  volume = {116},
  pages = {293--312},
  abstract = {We study gender differences in altruism by examining a modi\$\textbackslash{}backslash\$negéed dictator game with varying incomes and prices. Our results indicate that the question `which is the fair sex?' has a complicated answer\{\$\textbackslash{}backslash\$quotesinglbase\}Äîwhen altruism is expensive, women are kinder, but when it is cheap, men are more altruistic. That is, we \$\textbackslash{}backslash\$negéfind that the male and female `demand curves for altruism' cross, and that men are more responsive to price changes. Furthermore, men are more likely to be either perfectly selfish or perfectly seleflss, whereas women tend to be `equalitarians' who prefer to share even ... Second, this indicates a need for more attention to sex differences in experimental economics. If differ- ences appear with respect to altruism, they may appear in other behavior as well.23 This, in turn, means that researchers would be wise to assure that their experimental \$\textbackslash{}backslash\$negéndings are the result of economic incentives and not of varying sex compositions of their control and treatment groups. There are also potential applications of this result to the real world. If sex differences in altruism are truly present, as sug- gested by evidence from charitable giving and from restaurant tipping, then those who broker in altruism, such as charities and fundraisers, may \$\textbackslash{}backslash\$negénd it in their interests to learn more about these differences and to exploit them in their enterprises. For tax policy, sex difference in altruism may also indicate that the inci- dence of the tax deduction to charity may differ by sex as well, with men\{\$\textbackslash{}backslash\$quotesinglbase\}Äîand the charities they favor\{\$\textbackslash{}backslash\$quotesinglbase\}Äîgetting a greater share of the subsidy.},
  number = {1}
}

