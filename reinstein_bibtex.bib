@article{Levati2007,
  title = {Leading by {{Example}} in a {{Public Goods Experiment}} with {{Heterogeneity}} and {{Incomplete Information}}},
  author = {Levati, M Vittoria and Sutter, Matthias and van der Heijden, Eline},
  date = {2007},
  journaltitle = {Journal of Conflict Resolution},
  volume = {51},
  pages = {793--818},
  abstract = {We study the effects of leadership on the private provision of a public
good when group members are heterogeneously endowed. Leadership is
implemented as a sequential public goods game where one group member
contributes first and all the others follow. Our results show that
the presence of a leader increases average contribution levels but
less so than in case of homogeneous endowments. Leadership is almost
ineffective, though, if participants do not know the distribution
of endowments. Granting the leaders exclusion power does not lead
to significantly higher contributions.},
  keywords = {[philanthropy],endowments,experimental,fairness,incomplete information,leadership,public goods,reputation.,social giving,vcm},
  options = {useprefix=true}
}

