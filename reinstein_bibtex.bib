@article{liDoesWindfallMoney2018,
  title = {Does {{Windfall Money Encourage Charitable Giving}}? {{An Experimental Study}}},
  author = {Li, Huafang and Liang, Jie and Xu, Hui and Liu, Yingxi},
  date = {2018},
  journaltitle = {Voluntas},
  issn = {09578765},
  doi = {10.1007/s11266-018-9985-y},
  abstract = {Recently, increasing numbers of nonprofit studies have used experiments to understand individuals' charitable giving decisions. One significant gap between experimental settings and the real world is the way in which individuals earn the incomes that they use for charitable donations. This study examined the relationship between individuals' income sources and their charitable giving decisions. To do so, we conducted a laboratory experiment with 188 college students and asked them to donate with windfall money or with money earned from a real task, respectively. The findings showed that participants donated more to charities if their funds derived from windfall gains. Implications for conducting experiments and motivating donors are also discussed.}
}

