@article{Varian1994,
  title = {Sequential Contributions to Public Goods},
  author = {Varian, H R},
  date = {1994},
  journaltitle = {Journal of Public Economics},
  volume = {53},
  pages = {165--186},
  publisher = {{Elsevier}},
  abstract = {I examine games involving private contributions to a public good and
show that less of the public good will be supplied if agents move
sequentially than if they move simultaneously. If the agents bid
for the right to move first, the agent who values the public good
least will win. If each agent chooses the rate at which he will subsidize
the other agent's contributions, the subsidies that support the Lindahl
allocation are the unique equilibrium outcome. I also describe two
related subsidy-setting games that yield Lindahl allocations in n-person
games with general utility functions.},
  number = {2}
}

