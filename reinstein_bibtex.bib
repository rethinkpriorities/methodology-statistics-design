@article{carriganMythEthicalConsumer2001,
  ids = {Carrigan2001,Carrigan2001a,carriganMythEthicalConsumer2001a,carriganMythEthicalConsumer2001b},
  title = {The Myth of the Ethical Consumer–Do Ethics Matter in Purchase Behaviour?},
  author = {Carrigan, M and Attalla, A},
  date = {2001},
  journaltitle = {Journal of Consumer Marketing},
  volume = {18},
  pages = {560--577},
  publisher = {{Emerald Group Publishing Limited}},
  abstract = {Abstract: Marketing ethics and social responsibility are inherently controversial, and years of research continue to present conflicts and challenges for marketers on the value of a socially responsible approach to marketing activities. This article examines whether or not consumers care about ethical behaviour, and investigates the effect of good and bad ethical conduct on consumer purchase behaviour. Through focus group discussions it becomes clear that although we are more sophisticated as consumers today, this does not necessarily translate into behaviour which favours ethical companies and punishes unethical firms. The article concludes by some thoughts on how marketers might encourage consumers to engage in positive purchase behaviour in favour of ethical marketing.},
  keywords = {ethics,ft,FT,marketing},
  number = {7}
}

