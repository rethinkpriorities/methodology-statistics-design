@article{Eckel1998,
  title = {Are {{Women Less Selfish Than Men}}?: {{Evidence}} from {{Dictator Experiments}}},
  author = {Eckel, C C and Grossman, P},
  date = {1998-05},
  journaltitle = {The Economic Journal},
  volume = {108},
  pages = {726--735},
  publisher = {{Royal Economic Society}},
  issn = {0013-0133},
  url = {http://links.jstor.org/sici?sici=0013-0133(199805)108:448%3C726:AWLSTM%3E2.0.CO;2-7},
  abstract = {Research in social sciences other than economics indicates substantial differences in behaviour between men and women. The general conclusion drawn from this work is that women will be more socially-orientated (selfless), and men more individually-orientated (selfish). This paper reports the results of a double-anonymous dictator experiment designed to permit the emergence of basic gender differences in economic behaviour. Our results are intended to provide a baseline for further research. We find that women, on average, donate twice as much as men to their anonymous partners when any factors that might confound cooperation are eliminated.},
  keywords = {experiments; altruism; dictator games; philanthrop},
  number = {448}
}

