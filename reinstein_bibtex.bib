@article{Frey2004b,
  title = {Social Comparisons and Pro-Social Behavior: {{Testing}} Conditional Cooperation in a Field Experiment},
  author = {Frey, Bruno S. and Meier, Stephan},
  date = {2004},
  journaltitle = {American Economic Review},
  volume = {94},
  pages = {1717--1722},
  issn = {00028282},
  doi = {10.1257/0002828043052187},
  url = {papers3://publication/uuid/59EA7CEC-2288-44F3-BF35-21032C468844},
  abstract = {This paper presents evidence from a large-scale field experiment on conditional cooperation: people behave pro-socially depending on the pro-social behavior of other persons. When students were presented with the information that many others donated to two charitable funds at the University of Zurich in Switzerland, their willingness to contribute increased. This constitutes a first test of social interaction effects in charitable giving in a field experiment. The behavior resulting from conditional cooperation is consistent with at least three theoretical approaches: first, people may want to behave in an appropriate way and to conform to a social norm; second, people have some level of fairness preferences, such as reciprocity; or third, contributions by others may serve as a signal of the quality of the public good, or of the organization that provides the good. Results from previous experiments that attempt to discriminate among the various explanations are ambiguous. Some experimental studies indicate that conformity can explain conditional cooperation better than reciprocal considerations, while others come to the opposite conclusion. Yet other laboratory experiments find evidence supporting the third mechanism, that cooperative behavior of others is used as an indication of the quality of the public good.},
  isbn = {00028282},
  keywords = {conditional cooperation,donations,public goods,social comparisons},
  number = {5}
}

