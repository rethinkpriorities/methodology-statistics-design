@article{Gino2008,
  title = {Do We Listen to Advice Just Because We Paid for It? {{The}} Impact of Advice Cost on Its Use},
  author = {Gino, Francesca},
  date = {2008},
  journaltitle = {Organizational Behavior and Human Decision Processes},
  volume = {107},
  pages = {234--245},
  issn = {07495978},
  doi = {10.1016/j.obhdp.2008.03.001},
  abstract = {When facing a decision, people often rely on advice received from others. Previous studies have shown that people tend to discount others' opinions. Yet, such discounting varies according to several factors. This paper isolates one of these factors: the cost of advice. Specifically, three experiments investigate whether the cost of advice, independent of its quality, affects how people use advice. The studies use the Judge-Advisor System (JAS) to investigate whether people value advice from others more when it costs money than when it is free, and examine the psychological processes that could account for this effect. The results show that people use paid advice significantly more than free advice and suggest that this effect is due to the same forces that have been documented in the literature to explain the sunk costs fallacy. Implications for circumstances under which people value others' opinions are discussed. © 2008 Elsevier Inc. All rights reserved.},
  eprint = {21317316},
  eprinttype = {pmid},
  isbn = {0749-5978},
  keywords = {Advice taking,Information use,Judge-Advisor System,Paid-advice effect,Sunk costs},
  number = {2}
}

